# .env.example

# RPC Configuration
RPC_URL_MAINNET=https://mainnet.rpc.url
RPC_URL_TESTNET=https://testnet.rpc.url

# Wallet Settings
WALLET_PRIVATE_KEY=your_private_key
WALLET_ADDRESS=your_wallet_address

# Trading Parameters
TRADING_FEE=0.001
SLIPPAGE=0.01
TRADE_AMOUNT=0.1

# DEX Configuration
DEX_TYPE=Uniswap
DEX_ROUTER_ADDRESS=0x1234567890abcdef1234567890abcdef12345678

# Advanced Settings
ENABLE_LOGGING=true
LOG_LEVEL=debug
MAX_RETRY_ATTEMPTS=5
RETRY_DELAY=3